-- Title
Implementing the Printable interface
-- Input
interface Printable {
    function string: String;
}
monitor Monitor: Printable {
    function string: String { return "not a printable monitor"; }
}
-- Output
interface Printable {
    function string: String;
}
monitor Monitor: Printable {
    function string(value self: Monitor): String {
        return ("not a printable monitor": String)
    }
}
