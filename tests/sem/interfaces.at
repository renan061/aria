-- Title
Implementing the Dog interface
-- Input
interface Dog {
    function catch(ball: Integer);
    function bark: String;
}
monitor Monitor: Dog {
    function catch(ball: Integer) {}
    function bark: String { return "Woof!"; }
}
-- Output
interface Dog {
    function catch(value ball: Integer): Void;
    function bark: String;
}
monitor Monitor: Dog {
    function catch(value self: Monitor, value ball: Integer): Void {
    }
    function bark(value self: Monitor): String {
        return ("Woof!": String)
    }
}
