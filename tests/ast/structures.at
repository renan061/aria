Structure
-- Input
structure Person {
    value species = "homo sapiens";
    value name: String;
    variable age: Integer;
    variable planet = "earth";
}
-- Output
structure Person {
    value species: ? = ("homo sapiens")
    value name: String
    variable age: Integer
    variable planet: ? = ("earth")
}
