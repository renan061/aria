--------------------------------------------------------------------------------
-- Title
Expressions
-- Input
function main {
    // or
    a := true or false;
    // and
    a := true and false;
    // ==
    a := true == false;
    // !=
    a := true != false;
    // <=
    a := 1 <= 2;
    // >=
    a := 1 >= 2;
    // <
    a := 1 < 2;
    // >
    a := 1 > 2;
    // +
    a := 1 + 2;
    // -
    a := 1 - 2;
    // *
    a := 1 * 2;
    // /
    a := 1 / 2;
    // - (unary)
    a := -1;
    // not
    a := not true;
    // cast
    a := 1 as Integer;
    // literal boolean
    a := true;
    // literal false
    a := false;
    // literal integer
    a := 1;
    // literal float
    a := 1.5;
    // literal string
    a := "string";
    // literal array
    a := [1];
    a := [1, 2];
    a := [1, 2, 3];
    a := Immutable [-1, -2, -3, -4];
    // variable
    a := b;
    // function call
    a := foo();
    // list comprehension
    a := [x | x in 0...5];
    a := [f(i) | i in f(x)...f(y)];
    // parenthesized
    a := (1 - 1);
}
-- Output
OK
