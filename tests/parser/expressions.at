-- Title
Miscellaneous
-- Input
function main {
    a := true or false or 1 > 2;
    a := not 1 <= 3 and false and 1 < 2 or 1 >= 2;
    a := (1 + 3) - 2 / -5;
    a := --5;
    a := not not false;
}
-- Output
OK
